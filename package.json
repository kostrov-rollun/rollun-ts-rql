{
  "name": "rollun-ts-rql",
  "version": "0.6.1",
  "description": "RQL query object and serializer written in TypeScript",
  "scripts": {
    "analyse": "node_modules/.bin/tslint --project ./tslint.json -e **/tests/**",
    "fix-style": "node_modules/.bin/tslint --fix --project ./tslint.json",
    "build": "node_modules/.bin/tsc && babel src --out-dir dist --extensions '.ts'",
    "run-tests": "node_modules/.bin/intern",
    "test": "npm run build && npm run run-tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollun-com/rollun-ts-rql.git"
  },
  "keywords": [
    "rql",
    "typescript",
    "rollun"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rollun-com/rollun-ts-rql/issues"
  },
  "homepage": "https://github.com/rollun-com/rollun-ts-rql#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "intern": "^4.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "locutus": "^2.0.10",
    "lodash": "^4.17.11"
  },
  "files": [
    "dist/*"
  ]
}
