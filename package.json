{
  "name": "rollun-ts-rql",
  "version": "0.5.1",
  "description": "RQL query object and serializer written in TypeScript",
  "scripts": {
    "analyse": "node_modules/.bin/tslint --project ./tslint.json -e **/tests/**",
    "fix-style": "node_modules/.bin/tslint --fix --project ./tslint.json",
    "build": "node_modules/.bin/tsc",
    "run-tests": "node_modules/.bin/intern",
    "run-rql-group-by-test": "node_modules/.bin/intern suites=tests/unit/GroupByNodeTest.ts",
    "run-rql-parser-test": "node_modules/.bin/intern suites=tests/unit/RqlParserTest.ts",
    "test": "npm run build && npm run run-tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryakwork/rollun-ts-rql.git"
  },
  "keywords": [
    "rql",
    "typescript",
    "rollun"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bryakwork/rollun-ts-rql/issues"
  },
  "homepage": "https://github.com/bryakwork/rollun-ts-rql#readme",
  "devDependencies": {
    "intern": "^4.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "locutus": "^2.0.10",
    "lodash": "^4.17.11"
  },
  "files": [
    "dist/*"
  ]
}
